<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record model="ir.ui.view" id="base_config_settings_ip_totp">
            <field name="name">base_config_settings_ip_totp</field>
            <field name="model">base.config.settings</field>
            <field name="inherit_id" ref="base_setup.view_general_configuration"/>
            <field name="arch" type="xml">
                <group name="google" position="before">
                    <group name="totp" string="Login Security">
                        <field name="authorized_ip_ids">
                            <tree editable="bottom">
                                <field name="name"/>
                            </tree>
                        </field>
                    </group>
                </group>
            </field>
        </record>

    </data>
</odoo>