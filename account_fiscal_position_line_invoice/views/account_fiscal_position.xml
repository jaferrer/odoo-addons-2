<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="compta_view_account_position_form" model="ir.ui.view">
            <field name="name">compta_view_account_position_form</field>
            <field name="model">account.fiscal.position</field>
            <field name="inherit_id" ref="account.view_account_position_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='auto_apply']" position="before">
                    <group>
                        <field name="visible_in_line"/>
                    </group>
                </xpath>
                <page name="account_mapping" position="attributes">
                    <attribute name="group">account.group_account_manager</attribute>
                </page>
            </field>
        </record>

    </data>
</odoo>