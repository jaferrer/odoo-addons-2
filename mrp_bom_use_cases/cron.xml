<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record model="ir.cron" id="cron_compute_use_case_count">
            <field name="name">Update number of use cases for products</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="active" eval="True"/>
            <field name="priority">5</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall">2015-10-01 03:00:00</field>
            <field name="numbercall">-1</field>
            <field name="doall"/>
            <field name="model">product.template</field>
            <field name="function">cron_compute_use_case_count</field>
            <field name="args"/>
        </record>

        <record model="ir.cron" id="cron_compute_father_line_ids">
            <field name="name">Update father lines for BOM lines</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="active" eval="True"/>
            <field name="priority">5</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall">2015-10-01 03:00:00</field>
            <field name="numbercall">-1</field>
            <field name="doall"/>
            <field name="model">mrp.bom.line</field>
            <field name="function">cron_compute_father_line_ids</field>
            <field name="args"/>
        </record>

    </data>
</odoo>