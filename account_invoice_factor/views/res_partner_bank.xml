<openerp>
    <data>
        <record model="ir.ui.view" id="egg_res_partner_bank_form">
            <field name="name">sirail_res_partner_bank_form</field>
            <field name="model">res.partner.bank</field>
            <field name="inherit_id" ref="base.view_partner_bank_form"/>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <group name="bank" position="after">
                    <group name="factor" string="Factor settings">
                        <field name="factor_email" />
                        <field name="factor_cga_account_number" />
                        <field name="factor_contract_number" />
                        <field name="factor_company_code" />
                        <field name="factor_account_number" />
                        <field name="factor_contract_type" />
                        <field name="factor_currency_id" />
                    </group>
                </group>
            </field>
        </record>
    </data>
</openerp>