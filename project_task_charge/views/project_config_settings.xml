<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="project_task_charge_config_settings_form" model="ir.ui.view">
            <field name="name">project_task_charge_config_settings_form</field>
            <field name="model">project.config.settings</field>
            <field name="inherit_id" ref="project.view_config_settings"/>
            <field name="arch" type="xml">
                <field name="module_rating_project" position="after">
                    <field name="calc_min_date"/>
                    <field name="calc_max_date"/>
                </field>
            </field>
        </record>
    </data>

    <data noupdate="1">
        <record model="ir.config_parameter" id="calc_max_date">
            <field name="key">calc_max_date</field>
            <field name="value">3</field>
        </record>
        <record model="ir.config_parameter" id="calc_in_date">
            <field name="key">calc_min_date</field>
            <field name="value">3</field>
        </record>
    </data>
</openerp>