#. module: training_base
#: model:mail.template,body_html:training_base.email_template_training_convocation_to_partner
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        Hello ${ctx.get('title_name') and ctx.get('title_name') + \" \" or \"\"}${ctx.get('partner_name')},\n"
"                        <br/><br/>\n"
"                        Here is your convocation for training of ${format_date(object.date)}.\n"
"                        <br/><br/>\n"
"                        Title: ${object.training_id.name}.\n"
"                        <br/><br/>\n"
"                        % if object.location_id.is_accessible_prm:\n"
"                        Location (accessible to people with reduced mobility): ${object.location_id.with_context(contact_address_for_training_mails=True).contact_address}.\n"
"                        % endif\n"
"                        % if not object.location_id.is_accessible_prm:\n"
"                        Location: ${object.location_id.with_context(contact_address_for_training_mails=True).contact_address}.\n"
"                        % endif\n"
"                        <br/>\n"
"                        % if object.location_id.is_remote_training_location and object.remote_url:\n"
"                        <br/>\n"
"                        Please use <a href=\"${object.remote_url}\">this link</a>\n"
"                        <br/>\n"
"                        % endif\n"
"                        <br/>\n"
"                        Hour: from ${object.formatted_start_hour} to ${object.formatted_end_hour}.\n"
"                        <br/><br/>\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        Bonjour ${ctx.get('title_name') and ctx.get('title_name') + \" \" or \"\"}${ctx.get('partner_name')},\n"
"                        <br/><br/>\n"
"                        Voici votre convocation pour la formation du ${format_date(object.date, lang_code='fr_FR')}.\n"
"                        <br/><br/>\n"
"                        Titre : ${object.training_id.name}.\n"
"                        <br/><br/>\n"
"                        % if object.location_id.is_accessible_prm:\n"
"                        Lieu (accessible aux personnes handicapées) : ${object.location_id.with_context(contact_address_for_training_mails=True).contact_address}.\n"
"                        % endif\n"
"                        % if not object.location_id.is_accessible_prm:\n"
"                        Lieu : ${object.location_id.with_context(contact_address_for_training_mails=True).contact_address}.\n"
"                        % endif\n"
"                        <br/>\n"
"                        % if object.location_id.is_remote_training_location and object.remote_url:\n"
"                        <br/>\n"
"                        Veuillez utiliser <a href=\"${object.remote_url}\">ce lien</a>\n"
"                        <br/>\n"
"                        % endif\n"
"                        <br/>\n"
"                        Horaires : de ${object.formatted_start_hour} à ${object.formatted_end_hour}.\n"
"                        <br/><br/>\n"
"                    </p>\n"
"                </div>\n"
"            "

