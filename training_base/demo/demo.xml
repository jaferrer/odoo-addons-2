        <record model="res.partner" id="attendee_2">
            <field name="firstname">Jean</field>
            <field name="lastname">Raspail</field>
            <field name="is_attendee" eval="True"/>
        </record>

        <record model="res.partner" id="attendee_3">
            <field name="firstname">George</field>
            <field name="lastname">Orwell</field>
            <field name="is_attendee" eval="True"/>
        </record>

        <record model="res.partner" id="attendee_4">
            <field name="firstname">Jules</field>
