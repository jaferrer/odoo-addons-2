<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="delivery.carrier.provider" id="schenker">
        <field name="name">Schenker</field>
        <field name="active" eval="True"/>
        <field name="carrier">schenker</field>
        <field name="image" type="base64" file="delivery_tracking_schenker/static/img/schenker_logo.png"/>
    </record>

    <record model="product.product" id="schenker_delivery_product">
        <field name="name">Delivery Carrier Schenker</field>
        <field name="default_code">schenker</field>
        <field name="type">service</field>
        <field name="sale_ok" eval="True"/>
        <field name="purchase_ok" eval="False"/>
        <field name="active" eval="False"/>
    </record>
</odoo>