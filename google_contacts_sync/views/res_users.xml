<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="res_users_google_contacts_form">
            <field name="name">res.users.google.contacts.form</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[group[field[@name='signature']]]" position="after">
                    <page string="Google Contacts">
                        <group>
                            <field name="google_contacts_rtoken"/>
                            <field name="google_contacts_token"/>
                            <field name="google_contacts_token_validity"/>
                            <field name="google_contacts_last_sync_date"/>
                            <field name="contact_synchronization"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>