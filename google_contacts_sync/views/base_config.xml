<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="settings_google_contact_sync_view_form" model="ir.ui.view">
            <field name="name">settings.google.contact.sync</field>
            <field name="model">base.config.settings</field>
            <field name="inherit_id" ref="base_setup.view_general_configuration"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='google']" position="after">
                    <group>
                        <label for="id" string="Contacts Google"/>
                        <div>
                            <div>
                                <label for="google_contacts_client_id"/>
                                <field name="google_contacts_client_id" class="oe_inline"/>
                            </div>
                            <div>
                                <label for="google_contacts_client_secret"/>
                                <field name="google_contacts_client_secret" class="oe_inline" password="True"/>
                            </div>
                        </div>
                    </group>
                </xpath>
            </field>
        </record>

    </data>
</odoo>